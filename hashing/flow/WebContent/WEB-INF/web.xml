<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
         id="WebApp_ID" version="3.1">
  <display-name>flow</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>

  <!-- EncryptionServlet Definition -->
  <servlet>
    <servlet-name>EncryptionServlet</servlet-name>
    <servlet-class>EncryptionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EncryptionServlet</servlet-name>
    <url-pattern>/EncryptionServlet</url-pattern>
  </servlet-mapping>

  <!-- DecryptionServlet Definition -->
  <servlet>
    <servlet-name>DecryptionServlet</servlet-name>
    <servlet-class>DecryptionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DecryptionServlet</servlet-name>
    <url-pattern>/DecryptionServlet</url-pattern>
  </servlet-mapping>
</web-app>
